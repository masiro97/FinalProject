    function ajaxReq2(tableNum){
		
		 var xhttp = new XMLHttpRequest();
		  xhttp.onreadystatechange = function() {
		    if (this.readyState == 4 && this.status == 200) {
		    	//innerHTML - jsìì html tagë¥¼ íë©´ì ì¶ë ¥í  ë ì¬ì©ëë ìì±
		    	//ì£¼ìì¬í­ : divì p tagì ë°ìëë ìì±
		    	//innerTextë tagë°ì ìë¨
		    	if(tableNum == 1) document.getElementById("userCount").innerHTML = this.responseText;
		    	else if(tableNum == 2) document.getElementById("recordCount").innerHTML = this.responseText;
		    	
		    }
		  };
		  
		  var executeString;
		  if (tableNum == 1) executeString = "scont?command=userCount";
		  else if(tableNum == 2) executeString = "scont?command=recordCount";
		  else executeString = "scont?command=fail";
		  xhttp.open("GET", executeString, true);
